#include "os.h"
int running = 1;

void OS_Init(){
	// Initialize PPP and PPPMax[]
	PPPLen 				= MAXPROCESS;
	int PPP[PPPLen] 	= { 0 };
	int PPPMax[PPPLen] 	= { 0 };
}

void OS_Start(){

	while (running){
		//OS is running do os stuff here.
	}
}

void OS_Abort(){
	//will make the OS exit.
	running = 0;
}
