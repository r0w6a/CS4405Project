


#include <stdio.h>
#include "os.h"

int main()
{
	// This has to be first
	OS_init();

	// Initialize PPP and PPPMax[]
	PPPLen 				= MAXPROCESS;
	int PPP[PPPLen] 	= { 0 };
	int PPPMax[PPPLen] 	= { 0 };


	// here we can only call OS_Create(), OS_InitSem(), and OS_InitFiFo()


	// Boot the OS
	OS_Start(); // This never returns?

	return 0;
}

